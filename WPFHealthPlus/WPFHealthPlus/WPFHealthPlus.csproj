<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\DiaryIcon.png" />
    <None Remove="Icons\GoalIcon.png" />
    <None Remove="Icons\HealthPlusLogo.png" />
    <None Remove="Icons\HealtPlusWhite.png" />
    <None Remove="Icons\LogoutIcon.png" />
    <None Remove="Icons\MoreIcon.png" />
    <None Remove="Icons\ProfileIcon.png" />
    <None Remove="Icons\ProfileIconButton.png" />
    <None Remove="Icons\SumCaloriesIcon.png" />
    <None Remove="Icons\UserListManager.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignThemes" Version="4.9.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="HealthPlusApiService">
      <HintPath>..\..\..\HealthPlusProject-master\HealthPlusProject-master\HealthPlusApiService\bin\Debug\net8.0\HealthPlusApiService.dll</HintPath>
    </Reference>
    <Reference Include="Model">
      <HintPath>..\..\..\HealthPlusProject-master\HealthPlusProject-master\HealthPlusProject\bin\Debug\net7.0\Model.dll</HintPath>
    </Reference>
    <Reference Include="ViewModel">
      <HintPath>..\..\..\HealthPlusProject-master\HealthPlusProject-master\ViewModel\bin\Debug\net7.0\ViewModel.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icons\DiaryIcon.png" />
    <Resource Include="Icons\GoalIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\HealthPlusLogo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\HealtPlusWhite.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\LogoutIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\MoreIcon.png" />
    <Resource Include="Icons\ProfileIcon.png" />
    <Resource Include="Icons\ProfileIconButton.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\SumCaloriesIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\UserListManager.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

